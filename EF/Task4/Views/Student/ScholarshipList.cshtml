@model IEnumerable<Task4.Models.Database.Student>

@{
    ViewBag.Title = "ScholarshipList";
}

<h2>Scholarship Student List</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Id_No)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Cgpa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Dob)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Id_No)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Cgpa)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Dob)
        </td>
       
    </tr>
}

</table>
